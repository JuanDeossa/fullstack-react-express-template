#postgres DB ( USER y PASSWORD necesarias para acceder a la db desde una terminal o cliente)
POSTGRES_USER=
POSTGRES_PASSWORD=
POSTGRES_DB= # Nombre de la base de datos
POSTGRES_HOST=db # tiene que llamarse igual que el servicio de la db en el docker-compose.yml
POSTGRES_PORT=5432
POSTGRES_SCHEMA=public

#postgres-db url conexion
DATABASE_URL: postgres://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/${POSTGRES_DB}?schema=${POSTGRES_SCHEMA}

#jwt
ACCESS_TOKEN_SECRET=
REFRESH_TOKEN_SECRET=

#frontend
FRONTEND_BASE_URL=http://localhost:5173 # la misma url base de tu frontend